<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#BEBEBE"
    android:orientation="vertical">

    <TextView
        android:id="@+id/nota_no"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="6dp"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:textStyle="bold"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tess1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/nota_no"
        android:maxLines="1"
        android:padding="3dp"
        android:text="Plat No."
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/plat_no_set"
        android:layout_width="577dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/tess1"
        android:maxLines="1"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tess2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tess1"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/nota_no"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:text="Montir"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/montir"
        android:layout_width="577dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/plat_no_set"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/tess2"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tess3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tess2"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/nota_no"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:text="Tanggal"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tanggal"
        android:layout_width="577dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/montir"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/tess3"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tess6"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tess3"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/nota_no"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:text="Masalah"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/masalah"
        android:layout_width="577dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tanggal"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/tess3"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tess4"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tess6"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/nota_no"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:text="Keluhan"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/keluhan"
        android:layout_width="378dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/masalah"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/tess4"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tess5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tess4"
        android:layout_marginLeft="-7dp"
        android:layout_toRightOf="@id/nota_no"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:text="Catatan"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/catatanservis"
        android:layout_width="577dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/keluhan"
        android:layout_marginLeft="-7dp"
        android:layout_marginTop="0dp"
        android:layout_toRightOf="@id/tess5"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="3dp"
        android:textColor="#000000"
        android:textColorHighlight="@color/colorPrimaryDark"
        android:textColorLink="@color/colorPrimary"
        android:textCursorDrawable="?attr/actionModeSplitBackground"
        android:textSize="18sp"
        android:visibility="visible" />

</RelativeLayout>
