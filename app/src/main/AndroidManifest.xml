<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.automech">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/automech_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/automech_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:configChanges="orientation|screenSize">
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainMenu" />
        <activity android:name=".BuatPemilik" />
        <activity android:name=".LihatPemilik" />
        <activity android:name=".UbahPemilik" />
        <activity android:name=".BuatMotor" />
        <activity android:name=".LihatMotor" />
        <activity android:name=".UbahMotor" />
        <activity android:name=".BuatServis"/>
        <activity android:name=".LihatServis" />
        <activity android:name=".UbahServis" />
        <activity android:name=".BuatMotorBaru"/>
        <activity android:name=".BuatServisBaru"/>
    </application>

</manifest>